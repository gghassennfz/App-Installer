{
  "name": "installer",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron:dev": "concurrently \"cross-env BROWSER=none npm run dev\" \"wait-on tcp:5174 && electron .\"",
    "electron:build": "vite build && electron-builder",
    "electron:preview": "electron ."
  },
  "build": {
    "appId": "com.developer.toolsinstaller",
    "productName": "Developer Tools Installer",
    "directories": {
      "output": "dist_electron"
    },
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "@electron/remote": "^2.1.2",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^35.0.0",
    "electron-builder": "^25.1.8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "wait-on": "^8.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "vite": "^6.2.0"
  }
}
